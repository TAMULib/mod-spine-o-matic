<?xml version="1.0" encoding="UTF-8"?>
<item link="">
  <bib_data>
    <title th:text="${title}"></title>
  </bib_data>
  <holding_data link="">
    <call_number_prefix th:text="${call_number_prefix}"></call_number_prefix>
    <call_number th:text="${call_number}"></call_number>
    <call_number_type th:attr="desc=${call_number_type_desc}" th:text="${call_number_type}"></call_number_type>
  </holding_data>
  <item_data>
    <enumeration th:text="${enumeration}"></enumeration>
    <chronology th:text="${chronology}"></chronology>
    <description></description>
    <library th:attr="desc=${library_description}" th:text="${library_code}"></library>
    <location th:attr="desc=${location_name}" th:text="location_code"></location>
    <location_gloss th:text="${location_gloss}"></location_gloss>
  </item_data>
</item>
